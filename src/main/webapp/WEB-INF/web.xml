<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Store SportWear</display-name>


    <context-param>
        <param-name>JDBC_URL</param-name>
        <param-value>"jdbc:mysql://localhost:3306/sport?useUnicode=true&amp;serverTimezone=UTC"</param-value>
    </context-param>
    <context-param>
        <param-name>JDBC_login</param-name>
        <param-value>root</param-value>
    </context-param>
    <context-param>
        <param-name>JDBC_password</param-name>
        <param-value>root</param-value>
    </context-param>


</web-app>